  "flex-layout.row#obra": {
    "children": [
      "flex-layout.col#obra"
    ]
  },

  "flex-layout.col#obra": {
    "children": [
      "rich-text#itemName",
      "rich-text#itemDescriptionDesktop",
      "sandbox#video",
      "rich-text#itemDescriptionMobile",
      "flex-layout.row#itemPhotos"
    ]
  },
  "flex-layout.row#itemPhotos": {
    "children": [
      "image#itemImage1",
      "image#itemImage2",
      "image#itemImage3",
      "image#itemImage4",
      "image#itemImage5",
      "image#itemImage6",
      "image#itemImage7",
      "image#itemImage8",
      "image#itemImage9",
      "image#itemImage10",
      "image#itemImage11",
      "image#itemImage12"
    ],
    "props": {
      "blockClass": "deepGallery"
    }
  },
  "rich-text#itemName": {
    "props": {
      "text": "Nome da Obra",
      "blockClass": "itemName"
    }
  },
  "sandbox#video": {
    "props": {
      "width": "100%",
      "height": "auto",
      "initialContent": "Component sandbox para inserção do vídeio via Editor de Layout"
    }
  },
  "rich-text#itemDescriptionDesktop": {
    "props": {
      "text": "Descrição da Obra",
      "blockClass": "itemDescriptionDesktop"
    }
  },
  "rich-text#itemDescriptionMobile": {
